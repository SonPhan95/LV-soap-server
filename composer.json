{
    "name": "sekison/laravel-soap-server",
    "description": "Laravel SOAP service server develop by kduma and forked by son",
    "keywords": [
        "sekison",
        "laravel-soap-server"
    ],
    "homepage": "https://github.com/SonPhan95/LV-soap-server",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "git@krystian.duma.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "laminas/laminas-soap": "^2.10",
        "ext-soap": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "KDuma\\SoapServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KDuma\\SoapServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KDuma\\SoapServer\\LaravelSoapServerServiceProvider"
            ]
        }
    }
}
