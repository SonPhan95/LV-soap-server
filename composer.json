{
    "name": "kduma/laravel-soap-server",
    "description": "Laravel SOAP service server",
    "keywords": [
        "kduma",
        "laravel-soap-server"
    ],
    "homepage": "https://github.com/kduma/laravel-soap-server",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "git@krystian.duma.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-soap": "^2.8",
        "ext-soap": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kduma\\SoapServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kduma\\SoapServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kduma\\SoapServer\\LaravelSoapServerServiceProvider"
            ]
        }
    }
}
